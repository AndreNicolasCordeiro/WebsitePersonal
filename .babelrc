{
"presets": ["next/babel", "@babel/preset-typescript"],
"plugins": [
    [
    "babel-plugin-styled-components",
    
    {
        "ssr": true,
        "displayName": true
    }
],
["@babel/plugin-proposal-class-properties"]
],

"env": {
    "test": {
        "plugins": [
            [
            "babel-plugin-styled-components",
            {
                "ssr": false,
                "displayName" : false
            }
        ]
    ]
}
}
}